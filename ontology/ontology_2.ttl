@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dpv: <http://dockerpedia.inf.utfsm.cl/vocab#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix c2t: <https://w3id.org/okn/o/c2t#> .
@prefix dct: <https://purl.org/dc/terms/> .
@prefix sd: <httpsd: htttp://w3id.org/okn/o/sd#> .

dpv: a owl:Ontology ;
    dc:description "Ontology code created to illustrate the project Containers To Triples"@en ;
    dc:creator "Lara Herrera, David Lázaro y Adrián Pascual" ;
    dc:created "July, 2022"@en ;
    dc:title "Containers To Triples"@en ;
    dc:licenseURI <https://opensource.org/licenses/Apache-2.0> ;
    dc:licenseName "Apache 2.0" ;
    dc:preferredNamespaceUri "https://w3id.org/okn" ;
    dc:citation "Cite this vocabulary as: L. Herrera, D. Lázaro, A. Pascual. Containers To Triples"@en .


### dpv:hasImage
dpv:hasImage rdf:type owl:ObjectProperty ;
		rdfs:domain dpv:DockerFile ;
		rdfs:comment "A docker file has software images" ;
		rdfs:range dpv:Image ;
		rdfs:label "hasImage" .

### c2t:hasLayer
c2t:hasLayer rdf:type owl:ObjectProperty ;
		rdfs:domain dpv:Image ;
		rdfs:comment "Several packages may have been installed on an image" ;
		rdfs:range dpv:ImageLayer ;
		rdfs:label "hasLayer" .

### dpv:isInstalledOn
dpv:isInstalledOn rdf:type owl:ObjectProperty ;
		rdfs:domain dpv:PackageVersion ;
		rdfs:comment "One OS may have installed several software packages on it" ;
		rdfs:range dpv:Image ;
		rdfs:isDefinedBy "http://dockerpedia.inf.utfsm.cl/vocab#isInstalledOn" ;
		rdfs:label "isInstalledOn" .

### c2t:distributionOf
c2t:distributionOf rdf:type owl:ObjectProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "A distribution is an operating system made from a software collection that includes the kernel and, often, a package management system" ;
		rdfs:range dpv:OperatingSystem ;
		rdfs:label "distributionOf" .

### c2t:isOperativeSystemOf
c2t:isOperativeSystemOf rdf:type owl:ObjectProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "An image could have an operative system" ;
		rdfs:range dpv:Image ;
		rdfs:label "isOperativeSystemOf" .

### dpv:isInstalledOn
dpv:isInstalledOn rdf:type owl:ObjectProperty ;
		rdfs:range dpv:ImageLayer ;
		rdfs:label "isInstalled on" .

#################################################################
#    Data Properties
#################################################################

### dct:identifier
dct:identifier rdf:type owl:DatatypeProperty ;
		rdfs:label "identifier" ;
		rdfs:domain dpv:Image ;
		rdfs:comment "An unambiguous reference to the resource within a given context" ;
    	rdfs:isDefinedBy  "http://purl.org/dc/terms/identifier" .
		

### dpv:tag
dpv:tag rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:Image ;
		rdfs:comment "Image’s versions are identified by its tag" ;
		rdfs:label "tag" .

### c2t:architecture
c2t:architecture rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:Image ;
		rdfs:comment "The architecture for which the image is built. For example, amd64" ;
		rdfs:label "architecture" .

### sd:dateCreated
sd:dateCreated rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:Image ;
		rdfs:comment "Date when a software component was created" ;
		rdfs:label "dateCreated" .

### dpv:size
dpv:size rdf:type owl:DatatypeProperty ;
		rdfs:comment "An image/package has a size" ;
		rdfs:domain dpv:Image, dpv:PackageVersion ;
    	rdfs:isDefinedBy "http://dockerpedia.inf.utfsm.cl/vocab#size" ;
		rdfs:label "size" .

### sd:name
sd:name rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:OperatingSystem, dpv:PackageVersion ;
		rdfs:comment "Name of the resource" ;
    	rdfs:isDefinedBy "https://w3id.org/okn/o/sd#name" ;
		rdfs:label "name" .

### sd:programmingLanguage
sd:programmingLanguage rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:PackageVersion ;
		rdfs:comment "Language used to code a software component" ;
		rdfs:label "programmingLanguage" .

### c2t:packageURL
c2t:packageURL rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:PackageVersion ;
		rdfs:comment "A package version may be associated to an URL" ;
		rdfs:label "packageURL" .

### sd:hasVersionId
sd:hasVersionId rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "Identifier of the version of this software" ;
    	rdfs:isDefinedBy "https://w3id.org/okn/o/sd#hasVersionId" ;  
		rdfs:label "hasVersionId" .

### dct:publisher
dct:publisher rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:PackageVersion ;
		rdfs:comment "A package/operating system is published by the publisher" ;
		rdfs:label "publisher" .

### dct:license
dct:license rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "A package version may specify the version that is being used" ;
    	rdfs:isDefinedBy "https://purl.org/dc/terms/license" ;
		rdfs:label "license" .

### c2t:operativeSystemPackageVersion
c2t:operativeSystemPackageVersion rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:PackageVersion ;
		rdfs:comment "Version of an operative system package" ;
		rdfs:label "operativeSystemPackageVersion" .

### sd:description
sd:description rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "A description of a resource" ;
		rdfs:label "description" .

### sd:hasDocumentation
sd:hasDocumentation rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "Pointer to the documentation of the model" ;
		rdfs:label "hasDocumentation" .

### sd:issueTracker
sd:issueTracker rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "Pointer to the documentation of the model" ;
		rdfs:label "issueTracker" .

### sd:website
sd:website rdf:type owl:DatatypeProperty ;
		rdfs:domain dpv:OperatingSystem ;
		rdfs:comment "Website of the software" ;
		rdfs:label "website" .

#################################################################
#    Classes
#################################################################

### dpv:DockerFile
dpv:DockerFile rdf:type owl:Class ;
	rdfs:label "DockerFile" ;
    rdfs:comment "A file containing the instructions required to build an image (docker:SoftwareImage) in the Docker framework" .

### dpv:Image
dpv:Image rdf:type owl:Class ;
	rdfs:label "Image" ;
    rdfs:comment "A serialized copy of the state of a software stored in some non-volatile form such as a file" .

### dpv:ImageLayer
dpv:ImageLayer rdf:type owl:Class ;
	rdfs:label "ImageLayer" ;
    rdfs:comment "A layer of a Docker image are a set files generated from running a command from a DockerFile" .


### dpv:PackageVersion
dpv:PackageVersion rdf:type owl:Class ;
	rdfs:label "PackageVersion" ;
    rdfs:comment "Version of a software package" .

### dpv:OperatingSystem
dpv:OperatingSystem rdf:type owl:Class ;
	rdfs:label "OperatingSystem" ;
    rdfs:comment "A software operating system representation" .



